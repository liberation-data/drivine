{
  "compilerOptions": {
    "allowJs": false,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitThis": false,
    "suppressImplicitAnyIndexErrors": true,
    "strictPropertyInitialization": false,
    "target": "es2018",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "./dist",
    "paths": {
      "@/*": ["*"],
      "*": ["node_modules/*", "src/typings/*"]
    },
    "strict": true
  },
  "exclude": ["node_modules", "aurora", "build", "src/driver/agensgraph/**/*"]
}
